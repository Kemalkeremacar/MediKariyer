{
  "expo": {
    "name": "MediKariyer",
    "slug": "medikariyer",
    "version": "1.0.2",
    "orientation": "default",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "description": "Sağlık sektöründe çalışan doktorlar için iş bulma ve kariyer geliştirme platformu",
    "extra": {
      "EXPO_PUBLIC_API_BASE_URL": "https://mkapi.monassist.com/api/mobile",
      "EXPO_PUBLIC_PRIMARY_API_BASE_URL": "https://mkapi.monassist.com/api",
      "EXPO_PUBLIC_APP_ENV": "production",
      "EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS": "true",
      "EXPO_PUBLIC_SENTRY_DSN": "https://f80ca1e962d79bc709cb953067d3f82d@o4510787353116672.ingest.de.sentry.io/4510787359998032",
      "eas": {
        "projectId": "53bcb7fe-0588-4374-93e6-0bf24a59c003"
      }
    },
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.medikariyer.mobile",
      "buildNumber": "2",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Profil fotoğrafınızı güncellemek için galeri erişimine ihtiyacımız var.",
        "NSCameraUsageDescription": "Profil fotoğrafınızı çekmek için kamera erişimine ihtiyacımız var."
      }
    },
    "android": {
      "package": "com.medikariyer.mobile",
      "versionCode": 8,
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.VIBRATE",
        "android.permission.RECORD_AUDIO"
      ],
      "softwareKeyboardLayoutMode": "pan",
      "enableEdgeToEdge": true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "scheme": "medikariyer",
    "plugins": [
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      [
        "expo-notifications",
        {
          "color": "#ffffff",
          "sounds": []
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Profil fotoğrafınızı güncellemek için galeri erişimine ihtiyacımız var.",
          "cameraPermission": "Profil fotoğrafınızı çekmek için kamera erişimine ihtiyacımız var."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ]
  }
}
